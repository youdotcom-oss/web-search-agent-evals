{
  "type": "module",
  "scripts": {
    "build": "docker compose build",
    "check": "bun run format:check && bun run lint && bun run typecheck",
    "check:write": "bun run format && bun run lint:fix && bun run format:package",
    "clean": "rm -rf data/results/*/*.jsonl",
    "compare": "bunx @plaited/agent-eval-harness compare",
    "compare:all-statistical": "bun run compare --run claude-builtin:data/results/claude-code/builtin-test.jsonl --run claude-you:data/results/claude-code/you-test.jsonl --run codex-builtin:data/results/codex/builtin-test.jsonl --run codex-you:data/results/codex/you-test.jsonl --run droid-builtin:data/results/droid/builtin-test.jsonl --run droid-you:data/results/droid/you-test.jsonl --run gemini-builtin:data/results/gemini/builtin-test.jsonl --run gemini-you:data/results/gemini/you-test.jsonl --strategy statistical -o data/comparison-all-statistical-test.json",
    "compare:all-weighted": "bun run compare --run claude-builtin:data/results/claude-code/builtin-test.jsonl --run claude-you:data/results/claude-code/you-test.jsonl --run codex-builtin:data/results/codex/builtin-test.jsonl --run codex-you:data/results/codex/you-test.jsonl --run droid-builtin:data/results/droid/builtin-test.jsonl --run droid-you:data/results/droid/you-test.jsonl --run gemini-builtin:data/results/gemini/builtin-test.jsonl --run gemini-you:data/results/gemini/you-test.jsonl --strategy weighted -o data/comparison-all-weighted-test.json",
    "compare:builtin-agents": "bun run compare --run claude:data/results/claude-code/builtin-test.jsonl --run codex:data/results/codex/builtin-test.jsonl --run droid:data/results/droid/builtin-test.jsonl --run gemini:data/results/gemini/builtin-test.jsonl --strategy weighted -o data/comparison-builtin-agents-test.json",
    "compare:cli": "bun scripts/compare.ts",
    "compare:cli-full": "bun scripts/compare.ts --mode full",
    "compare:cli-statistical": "bun scripts/compare.ts --strategy statistical",
    "compare:you-agents": "bun run compare --run claude:data/results/claude-code/you-test.jsonl --run codex:data/results/codex/you-test.jsonl --run droid:data/results/droid/you-test.jsonl --run gemini:data/results/gemini/you-test.jsonl --strategy weighted -o data/comparison-you-agents-test.json",
    "format": "biome format --write",
    "format:check": "biome format",
    "format:package": "format-package --write",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "prepare": "git config core.hooksPath .hooks",
    "run": "bun scripts/run.ts",
    "run:full": "bun run run --mode full",
    "run:test": "bun run run --mode test",
    "test": "bun test",
    "trials": "bun scripts/run-trials.ts",
    "trials:capability": "bun scripts/run-trials.ts --type capability",
    "trials:full": "bun scripts/run-trials.ts --mode full",
    "trials:regression": "bun scripts/run-trials.ts --type regression",
    "typecheck": "tsc --noEmit"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,md}": [
      "biome format --write",
      "biome lint --write"
    ],
    "package.json": [
      "format-package -w"
    ]
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@plaited/agent-eval-harness": "^0.6.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/bun": "latest",
    "@types/node": "^25.0.8",
    "bun-types": "latest",
    "format-package": "^7.0.0",
    "lint-staged": "^15.2.11",
    "typescript": "^5.9.3"
  }
}
